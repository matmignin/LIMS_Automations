{
    "npm.enableRunFromFolder": true,
    "editor.autoClosingBrackets": "never",
    "editor.acceptSuggestionOnEnter": "off",
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false,
        "**/Thumbs.db": false,
        ".gitignore": false,
        "*.ico": false,
        "*.vscode": false,
        "*/bin/*": false,
        "bin/": false,
        "lib/plugins/": false,
        "lib/*.code-workspace": false,
        "lib/CL3/*": false,
        "lib/DebugWindow.AHK": false,
        "lib/Gdip_All.ahk": false,
        "lib/JSON.ahk": false,
        "lib/plugins/*": false,
        "lib/settings.ahk": false,
        "lib/Vis2.ahk": false,
        "lib/Windownames.ini": false,
        "*/Roaming/*": false,
        "*.json": false,
        "*.txt": false,
        "VBA Code/*": false,
        "lib/CL3": false,
        "lib/clip.ahk": false,
        "lib/Studio.ahk": false,
        "lib/Tooltip.ahk": false,
        "**/.vimrc": false
    },
    "explorerExclude.backup": null,
    "editor.hideCursorInOverviewRuler": true,
    "editor.hover.delay": 500,
    "autoHide.autoHideSideBar": false,
    "autoHide.autoHidePanel": false,
    "notebook.outline.showCodeCells": true
}